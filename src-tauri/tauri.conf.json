{
    "$schema": "https://schema.tauri.app/config/2",
    "productName": "Codex Manager",
    "version": "0.1.0",
    "identifier": "com.codex-manager.app",
    "build": {
        "beforeDevCommand": "npm run dev",
        "devUrl": "http://localhost:1420",
        "beforeBuildCommand": "npm run build",
        "frontendDist": "../dist"
    },
    "app": {
        "windows": [
            {
                "title": "Codex 账号管理器",
                "width": 1024,
                "height": 700,
                "center": true,
                "resizable": true,
                "minWidth": 1024,
                "minHeight": 500,
                "visible": true
            }
        ],
        "security": {
            "csp": "default-src 'self'; connect-src 'self' https://api.openai.com https://chatgpt.com https://dav.jianguoyun.com; style-src 'self' 'unsafe-inline'"
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ]
    }
}